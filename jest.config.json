{
  "setupFiles": [
    "<rootDir>/enzyme.config.js"
  ],
  "rootDir": ".",
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "moduleFileExtensions": [
    "js",
    "ts",
    "json",
    "tsx"
  ],
  "moduleNameMapper": {},
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/"
  ],
  "testEnvironment": "jsdom"
}
